<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar">
      <%= render 'partials/dashboard-nav' %>
    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <h1 class="page-header">Communities
        <%= link_to 'Add New', new_community_path, class: 'btn btn-default pull-right' %>
      </h1>






      <h2 class="sub-header">Section title</h2>
      <div class="table-responsive">



            <% for community in @communities %>

              <p><%= community.name %></p>
              <p><%= community.overview %></p>
              <p><%= community.school_title %></p>
              <p><%= community.highschool %></p>
              <p><%= community.middleschool %></p>
              <p><%= community.elementaryschool %></p>

            <% end %>
          

      </div>
    </div>
  </div>
</div>
